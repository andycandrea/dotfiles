# Unix
alias tlf='tail -f'
alias ln='ln -v'
alias mkdir='mkdir -p'
alias ...='../..'
alias -g G='| grep'
alias e="$EDITOR"
alias v="$VISUAL"

# Web frameworks
alias phs="mix phoenix.server"
alias pdc="mix ecto.create"
alias pdm="mix ecto.migrate"

alias ds="python ./manage.py runserver"

# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local
